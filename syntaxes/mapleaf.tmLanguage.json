{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "MAPLEAF",
    "fileTypes": [
        "mapleaf"
    ],
    "patterns": [{
            "name": "comment",
            "match": "(?<!\\\\)#.*"
        },
        {
            "match": "-?[0-9]+([,.][0-9]+)?(e[^\\s]*)?",
            "name": "constant.numeric"
        },
        {
            "match": "\\([\\s]*-?[0-9]+([,.][0-9]+)?(e[^\\s]*)?[\\s]+-?[0-9]+([,.][0-9]+)?(e[^\\s]*)?[\\s]+-?[0-9]+([,.][0-9]+)?(e[^\\s]*)?[\\s]*\\)",
            "name": "constant.numeric"
        },
        {
            "name": "string",
            "begin": "\"",
            "end": "\""
        },
        {
            "name": "string",
            "begin": "'",
            "end": "'"
        },
        {
            "name": "keyword.control",
            "match": "(!create|!replace|!removeKeysContaining|!include)\\b"
        },
        {
            "match": "(?<=!replace.*)with",
            "name": "keyword.control"
        },
        {
            "match": "(?<=!create.*)from",
            "name": "keyword.control"
        },
        {
            "match": "(?<=!create).*(?=from)",
            "name": "support.type.property-name"
        },
        {
            "match": "(?<=!include).*",
            "name": "support.type.property-name"
        },
        {
            "match": "[^\\s]+{",
            "name": "entity.name"
        },
        {
            "match": "^[\\s]*[^\\s{!]+[\\s]+",
            "name": "support.type.property-name"
        },
        {
            "match": "(?<=^[\\s]*[^\\s{!]+[\\s]+).*",
            "name": "string"
        }
    ],
    "scopeName": "text.mapleaf"
}